{
    "name": "nilportugues/validator",
    "description": "An efficient data validator for PHP",
    "keywords": ["input", "data", "validation", "validator", "nil portugues", "nilportugues"],
    "type": "library",
    "homepage": "http://nilportugues.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil Portugu√©s",
            "email": "contact@nilportugues.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "fiunchinho/phpunit-randomizer": "1.0.*@dev",
        "fabpot/php-cs-fixer": "dev-master",
        "nilportugues/php_backslasher": "~0.2"
    },
    "suggest": {
        "ext-mbstring": "Multibyte String Functions"
    },
    "autoload": {
        "psr-4": {
            "NilPortugues\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\NilPortugues\\Validator\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
